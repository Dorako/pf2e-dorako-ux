{
  "id": "pf2e-dorako-ux",
  "title": "PF2e Dorako UX",
  "description": "Tweaks to the Foundry UI",
  "url": "https://github.com/Dorako/pf2e-dorako-ux",
  "authors": [
    {
      "name": "Dorako",
      "email": "dirako@gmail.com",
      "url": "https://github.com/Dorako/pf2e-dorako-ux",
      "discord": "Dorako#0657"
    }
  ],
  "version": "1.0.0",
  "compatibility": {
    "minimum": "12",
    "verified": "12"
  },
  "relationships": {
    "systems": [
      {
        "id": "pf2e",
        "type": "system",
        "compatibility": {
          "minimum": "6.0.0",
          "verified": "6.0.0"
        }
      },
      {
        "id": "swade",
        "type": "system",
        "compatibility": {
          "minimum": "2.2.5",
          "verified": "2.2.5"
        }
      },
      {
        "id": "dnd5e",
        "type": "system",
        "compatibility": {
          "minimum": "2.2.5",
          "verified": "9.9.9"
        }
      }
    ],
    "recommends": [
      {
        "id": "pf2e-dorako-ui",
        "type": "module"
      }
    ]
  },
  "media": [
    {
      "type": "setup",
      "url": "modules/pf2e-dorako-ux/img/hero.webp",
      "loop": false,
      "flags": {}
    }
  ],
  "flags": {
    "hotReload": {
      "extensions": [".css", ".json"]
    }
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json",
      "flags": {}
    },
    {
      "lang": "fr",
      "name": "Français",
      "path": "languages/fr.json",
      "flags": {}
    },
    {
      "lang": "de",
      "name": "Deutsch",
      "path": "languages/de.json",
      "flags": {}
    },
    {
      "lang": "cn",
      "name": "中文（简体）",
      "path": "languages/zh_Hans.json",
      "flags": {}
    },
    {
      "lang": "zh-tw",
      "name": "正體中文",
      "path": "languages/zh-tw.json",
      "flags": {}
    },
    {
      "lang": "es",
      "name": "Español",
      "path": "languages/es.json",
      "flags": {}
    },
    {
      "lang": "pl",
      "name": "polski",
      "path": "languages/pl.json",
      "flags": {}
    },
    {
      "lang": "it",
      "name": "Italiano",
      "path": "languages/it.json",
      "flags": {}
    }
  ],
  "styles": ["styles/dorako-ux.css"],
  "esmodules": [
    "esmodules/settings/settings.js",
    "esmodules/message-hooks.js",
    "esmodules/dorako-ux/misc-hooks.js",
    "esmodules/dorako-ux/customization.js",
    "esmodules/dorako-ux/compact-ui.js",
    "esmodules/dorako-ux/npc-sheet.js",
    "esmodules/dorako-ux/radial-condition-hud.js",
    "esmodules/dorako-ux/sidebar-resizer.js"
  ],
  "changelog": "https://github.com/Dorako/pf2e-dorako-ux/blob/master/CHANGELOG.md",
  "download": "https://github.com/Dorako/pf2e-dorako-ux/archive/refs/tags/v1.11.3.zip",
  "manifest": "https://github.com/Dorako/pf2e-dorako-ux/releases/latest/download/module.json",
  "readme": "https://github.com/Dorako/pf2e-dorako-ux",
  "bugs": "https://github.com/Dorako/pf2e-dorako-ux/issues"
